<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>controller</servlet-name>
  	<display-name>controller</display-name>
  	<description></description>
  	<servlet-class>servlet.controller</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>sucesscontroller</servlet-name>
  	<display-name>sucesscontroller</display-name>
  	<description></description>
  	<servlet-class>servlet.sucesscontroller</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>controller</servlet-name>
  	<url-pattern>/controller</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>sucesscontroller</servlet-name>
  	<url-pattern>/sucesscontroller</url-pattern>
  </servlet-mapping>
</web-app>
